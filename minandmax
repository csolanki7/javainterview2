package arrays;

public class minimum {
	
	
public static void main ( String [] args ) {
		
		int a[] = { 1,0,4,4,5 };
		int min = a[0];  
		int max = a [0];
		for ( int i=0; i < a.length ; i++ ){
			
		 if(a[i] < min ) {
			 min = a[i]; 
		 }
			
		}
		System.out.println(min);


		for ( int i=0; i < a.length ; i++ ){
			
		 if(a[i] > max ) {
			 max = a[i]; 
		 }
			
		}
		System.out.println(max);
		
		
		
	}
 
}
